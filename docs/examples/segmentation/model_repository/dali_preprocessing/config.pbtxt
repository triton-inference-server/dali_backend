name: "dali_preprocessing"
backend: "dali"
max_batch_size: 16
input [
  {
    name: "encoded"
    data_type: TYPE_UINT8
    dims: [ -1 ]
  }
]

output [
  {
    name: "original"
    data_type: TYPE_UINT8
    dims: [ -1, -1, -1 ]
  },
  {
    name: "preprocessed"
    data_type: TYPE_FP32
    dims: [ -1, -1, -1 ]
  }
]

parameters [
  {
    key: "split_along_outer_axis",
    value: { string_value: "original:preprocessed" }
  }
]
